{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-bwlearn1"
		},
		"ABLB_bw_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ABLB_bw'"
		},
		"ADLS_bwlearn1adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_bwlearn1adls'"
		},
		"ASQL_bwlearn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_bwlearn'"
		},
		"ADLS_bwlearn1adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://bwlearnblob.dfs.core.windows.net"
		},
		"HTTP_lego_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-bwlearn1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ABLB_bw_connectionString": {
						"value": "[parameters('ABLB_bw_connectionString')]"
					},
					"ADLS_bwlearn1adls_accountKey": {
						"value": "[parameters('ADLS_bwlearn1adls_accountKey')]"
					},
					"ASQL_bwlearn_connectionString": {
						"value": "[parameters('ASQL_bwlearn_connectionString')]"
					},
					"ADLS_bwlearn1adls_properties_typeProperties_url": {
						"value": "[parameters('ADLS_bwlearn1adls_properties_typeProperties_url')]"
					},
					"HTTP_lego_properties_typeProperties_url": {
						"value": "[parameters('HTTP_lego_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}